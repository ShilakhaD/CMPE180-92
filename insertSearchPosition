class Solution {
    public int searchInsert(int[] nums, int target) {
        int length = nums.length;
        if(length==0) return 0;
        else if(target>nums[length-1]) return (length);
        for(int i=0; i<length; i++){
            
        if(target<=nums[i]) return i; 
            
        }
        return -1;
    }
}
